<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/idprenom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="157dp"
        android:layout_marginRight="157dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idnom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/idprenom"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:text="TextView" />


    <TextView
        android:id="@+id/idage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="151dp"
        android:layout_marginRight="151dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idsexe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/idage"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="57dp"
        android:layout_marginRight="57dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idtelephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/idage"
        android:layout_alignParentTop="true"
        android:layout_marginTop="125dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/idsexe"
        android:layout_alignTop="@+id/idtelephone"
        android:text="TextView" />

    <TextView
        android:id="@+id/idniveau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/idage"
        android:layout_alignParentTop="true"
        android:layout_marginTop="183dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/idfiliere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/idsexe"
        android:layout_alignTop="@+id/idniveau"
        android:text="TextView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="283dp"
        android:layout_above="@+id/idtelephone"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginBottom="-158dp"
        app:srcCompat="@drawable/ic_launcher_background" />

</RelativeLayout>